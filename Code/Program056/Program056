#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_010B2BF0 .scope module, "understand_disable_keyword" "understand_disable_keyword" 2 7;
 .timescale 0 0;
v010B3D38_0 .var/i "counter", 31 0;
S_010B3CB0 .scope begin, "while_loop" "while_loop" 2 15, 2 15, S_010B2BF0;
 .timescale 0 0;
    .scope S_010B2BF0;
T_0 ;
    %set/v v010B3D38_0, 0, 32;
    %end;
    .thread T_0;
    .scope S_010B2BF0;
T_1 ;
    %fork t_1, S_010B3CB0;
    %jmp t_0;
    .scope S_010B3CB0;
t_1 ;
T_1.0 ;
    %movi 8, 1, 2;
    %or/r 8, 8, 2;
    %jmp/0xz T_1.1, 8;
    %load/v 8, v010B3D38_0, 32;
    %mov 40, 39, 1;
    %addi 8, 1, 33;
    %set/v v010B3D38_0, 8, 32;
    %vpi_call 2 19 "$display", "Incrementing counter";
    %load/v 8, v010B3D38_0, 32;
    %cmpi/u 8, 10, 32;
    %jmp/0xz  T_1.2, 4;
    %disable S_010B3CB0;
T_1.2 ;
    %jmp T_1.0;
T_1.1 ;
    %end;
    .scope S_010B2BF0;
t_0 %join;
    %vpi_call 2 27 "$display", "Finished incrementing counter";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "Program056.v";
