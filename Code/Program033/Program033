#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_010F3C90 .scope module, "understand_reduction_operators" "understand_reduction_operators" 2 7;
 .timescale 0 0;
v010F3F68_0 .var "operand_1", 0 4;
v010F3D18_0 .net "operand_2", 0 4, C4<01110>; 1 drivers
    .scope S_010F3C90;
T_0 ;
    %movi 8, 18, 5;
    %set/v v010F3F68_0, 8, 5;
    %end;
    .thread T_0;
    .scope S_010F3C90;
T_1 ;
    %load/v 8, v010F3F68_0, 5;
    %and/r 8, 8, 5;
    %vpi_call 2 21 "$display", "&%b = %b", v010F3F68_0, T<8,1,u>;
    %load/v 8, v010F3F68_0, 5;
    %or/r 8, 8, 5;
    %vpi_call 2 22 "$display", "|%b = %b", v010F3F68_0, T<8,1,u>;
    %load/v 8, v010F3F68_0, 5;
    %xor/r 8, 8, 5;
    %vpi_call 2 23 "$display", "^%b = %b\012", v010F3F68_0, T<8,1,u>;
    %load/v 8, v010F3F68_0, 5;
    %nand/r 8, 8, 5;
    %vpi_call 2 25 "$display", "~&%b = %b", v010F3F68_0, T<8,1,u>;
    %load/v 8, v010F3F68_0, 5;
    %nor/r 8, 8, 5;
    %vpi_call 2 26 "$display", "~|%b = %b", v010F3F68_0, T<8,1,u>;
    %load/v 8, v010F3F68_0, 5;
    %xnor/r 8, 8, 5;
    %vpi_call 2 27 "$display", "~^%b = %b", v010F3F68_0, T<8,1,u>;
    %load/v 8, v010F3F68_0, 5;
    %xnor/r 8, 8, 5;
    %vpi_call 2 28 "$display", "^~%b = %b\012", v010F3F68_0, T<8,1,u>;
    %load/v 8, v010F3D18_0, 5;
    %and/r 8, 8, 5;
    %vpi_call 2 30 "$display", "&%b = %b", v010F3D18_0, T<8,1,u>;
    %load/v 8, v010F3D18_0, 5;
    %or/r 8, 8, 5;
    %vpi_call 2 31 "$display", "|%b = %b", v010F3D18_0, T<8,1,u>;
    %load/v 8, v010F3D18_0, 5;
    %xor/r 8, 8, 5;
    %vpi_call 2 32 "$display", "^%b = %b\012", v010F3D18_0, T<8,1,u>;
    %load/v 8, v010F3D18_0, 5;
    %nand/r 8, 8, 5;
    %vpi_call 2 34 "$display", "~&%b = %b", v010F3D18_0, T<8,1,u>;
    %load/v 8, v010F3D18_0, 5;
    %nor/r 8, 8, 5;
    %vpi_call 2 35 "$display", "~|%b = %b", v010F3D18_0, T<8,1,u>;
    %load/v 8, v010F3D18_0, 5;
    %xnor/r 8, 8, 5;
    %vpi_call 2 36 "$display", "~^%b = %b", v010F3D18_0, T<8,1,u>;
    %load/v 8, v010F3D18_0, 5;
    %xnor/r 8, 8, 5;
    %vpi_call 2 37 "$display", "^~%b = %b", v010F3D18_0, T<8,1,u>;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
