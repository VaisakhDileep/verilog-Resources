S_010F3C70 .scope module, "demo_net_stimulus" "demo_net_stimulus" 2 40;
 .timescale 0 0;
S_01293C08 .scope module, "d_n" "demo_net" 2 41, 2 56, S_010F3C70;
 .timescale 0 0;
v010F3CF8_0 .var "register_1", 0 0;
v01293C90_0 .net "wire_1", 0 0, v010F3CF8_0; 1 drivers
S_010F30E0 .scope module, "demo_register_stimulus" "demo_register_stimulus" 2 7;
 .timescale 0 0;
S_010F2D58 .scope module, "d_r" "demo_register" 2 8, 2 23, S_010F30E0;
 .timescale 0 0;
v01293CE8_0 .var "register_1", 0 0;
v010F2DE0_0 .var "register_2", 0 0;
S_010F3168 .scope module, "understand_force_release_keywords" "understand_force_release_keywords" 2 70;
 .timescale 0 0;
    .scope S_01293C08;
T_0 ;
    %set/v v010F3CF8_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_01293C08;
T_1 ;
    %delay 50, 0;
    %delay 1, 0;
    %set/v v010F3CF8_0, 1, 1;
    %end;
    .thread T_1;
    .scope S_010F3C70;
T_2 ;
    %delay 50, 0;
    %force/v v01293C90_0, 0, 1;
    %delay 2, 0;
    %vpi_call 2 48 "$display", "d_n.wire_1: %b\012", v01293C90_0;
    %release/net v01293C90_0, 0, 1;
    %delay 4, 0;
    %vpi_call 2 52 "$display", "d_n.wire_1: %b", v01293C90_0;
    %end;
    .thread T_2;
    .scope S_010F2D58;
T_3 ;
    %set/v v01293CE8_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_010F2D58;
T_4 ;
    %set/v v010F2DE0_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_010F2D58;
T_5 ;
    %load/v 8, v010F2DE0_0, 1;
    %cassign/v v01293CE8_0, 8, 1;
    %cassign/link v01293CE8_0, v010F2DE0_0;
    %delay 1, 0;
    %set/v v010F2DE0_0, 1, 1;
    %vpi_call 2 31 "$display", "register_1    : %b", v01293CE8_0;
    %delay 5, 0;
    %set/v v010F2DE0_0, 0, 1;
    %delay 1, 0;
    %set/v v010F2DE0_0, 1, 1;
    %vpi_call 2 36 "$display", "register_1    : %b", v010F2DE0_0;
    %end;
    .thread T_5;
    .scope S_010F30E0;
T_6 ;
    %force/v v01293CE8_0, 0, 1;
    %delay 2, 0;
    %vpi_call 2 13 "$display", "d_r.register_1: %b\012", v01293CE8_0;
    %release/reg v01293CE8_0, 0, 1;
    %delay 1, 0;
    %vpi_call 2 17 "$display", "d_r.register_1: %b\012", v01293CE8_0;
    %delay 5, 0;
    %vpi_call 2 19 "$display", "d_r.register_1: %b\012", v01293CE8_0;
    %end;
    .thread T_6;
