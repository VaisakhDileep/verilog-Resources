    %vpi_call 2 22 "$display", v011D2B98_0;
    %vpi_call 2 24 "$display", "string_1: %d", v011D2B98_0;
    %vpi_call 2 26 "$display", "string_1: %s\012", v011D2B98_0;
    %vpi_call 2 37 "$monitor", $time, " string_1: %s", v011D2B98_0;
    %delay 10, 0;
    %movi 8, 1935764328, 32;
    %movi 40, 5661033, 32;
    %movi 72, 0, 32;
    %movi 104, 0, 24;
    %set/v v011D2B98_0, 8, 120;
    %delay 15, 0;
    %movi 8, 10, 32;
    %set/v v011D04D8_0, 8, 32;
    %vpi_call 2 43 "$monitoroff";
    %delay 20, 0;
    %movi 8, 1768714087, 32;
    %movi 40, 5662066, 32;
    %movi 72, 0, 32;
    %movi 104, 0, 24;
    %set/v v011D2B98_0, 8, 120;
    %vpi_call 2 47 "$monitoron";
    %delay 25, 0;
    %movi 8, 842019378, 120;
    %set/v v011D2B98_0, 8, 120;
    %vpi_call 2 51 "$finish";
    %end;
    .thread T_0;
