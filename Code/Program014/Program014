:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_010F2B10 .scope module, "understand_define" "understand_define" 2 7;
 .timescale 0 0;
v010F2B98_0 .var "word", 31 0;
    .scope S_010F2B10;
T_0 ;
    %vpi_call 2 23 "$display", "WORD_SIZE: ", 7'sb0100000, "\012";
    %vpi_call 2 25 "$stop";
    %ix/load 0, 0, 0;
    %set/x0 v010F2B98_0, 1, 1;
    %vpi_call 2 29 "$display";
    %vpi_call 2 30 "$display", "WORD_REG: %b", v010F2B98_0;
    %end;
    .thread T_0;
