#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01247C10 .scope module, "understand_bitwise_operators" "understand_bitwise_operators" 2 7;
 .timescale 0 0;
v01243F40_0 .var "operand_1", 0 4;
v01247A88_0 .var "operand_2", 0 4;
    .scope S_01247C10;
T_0 ;
    %mov 8, 3, 1;
    %movi 9, 11, 4;
    %set/v v01243F40_0, 8, 5;
    %end;
    .thread T_0;
    .scope S_01247C10;
T_1 ;
    %mov 8, 2, 1;
    %mov 9, 3, 1;
    %movi 10, 6, 3;
    %set/v v01247A88_0, 8, 5;
    %end;
    .thread T_1;
    .scope S_01247C10;
T_2 ;
    %load/v 8, v01243F40_0, 5;
    %inv 8, 5;
    %vpi_call 2 19 "$display", "~%b = %b", v01243F40_0, T<8,5,u>;
    %load/v 8, v01247A88_0, 5;
    %inv 8, 5;
    %vpi_call 2 20 "$display", "~%b = %b\012", v01247A88_0, T<8,5,u>;
    %load/v 8, v01243F40_0, 5;
    %load/v 13, v01247A88_0, 5;
    %and 8, 13, 5;
    %vpi_call 2 22 "$display", "%b  & %b = %b", v01243F40_0, v01247A88_0, T<8,5,u>;
    %load/v 8, v01243F40_0, 5;
    %load/v 13, v01247A88_0, 5;
    %or 8, 13, 5;
    %vpi_call 2 23 "$display", "%b  | %b = %b", v01243F40_0, v01247A88_0, T<8,5,u>;
    %load/v 8, v01243F40_0, 5;
    %load/v 13, v01247A88_0, 5;
    %xor 8, 13, 5;
    %vpi_call 2 24 "$display", "%b  ^ %b = %b", v01243F40_0, v01247A88_0, T<8,5,u>;
    %load/v 8, v01243F40_0, 5;
    %load/v 13, v01247A88_0, 5;
    %xnor 8, 13, 5;
    %vpi_call 2 25 "$display", "%b ~^ %b = %b", v01243F40_0, v01247A88_0, T<8,5,u>;
    %load/v 8, v01243F40_0, 5;
    %load/v 13, v01247A88_0, 5;
    %xnor 8, 13, 5;
    %vpi_call 2 26 "$display", "%b ^~ %b = %b", v01243F40_0, v01247A88_0, T<8,5,u>;
    %end;
    .thread T_2;
