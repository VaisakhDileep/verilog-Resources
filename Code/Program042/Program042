v011A7DD0_0 .var "temp_reg_variable", 0 4;
    .scope S_011A8808;
T_0 ;
    %movi 8, 2, 2;
    %set/v v011AB4C8_0, 8, 2;
    %end;
    .thread T_0;
    .scope S_011A8808;
T_1 ;
    %movi 8, 1, 2;
    %set/v v011A7800_0, 8, 2;
    %end;
    .thread T_1;
    .scope S_011A8808;
T_2 ;
    %delay 1, 0;
    %vpi_call 2 36 "$display", "time: %-2d", $time;
    %delay 2, 0;
    %vpi_call 2 38 "$display", "time: %-2d", $time;
    %load/v 8, v011AB4C8_0, 2;
    %ix/get 0, 8, 2;
    %delayx 0;
    %vpi_call 2 40 "$display", "time: %-2d", $time;
    %load/v 8, v011AB4C8_0, 2;
    %load/v 10, v011A7800_0, 2;
    %add 8, 10, 2;
    %ix/get 0, 8, 2;
    %delayx 0;
    %vpi_call 2 42 "$display", "time: %-2d\012", $time;
    %movi 8, 3, 5;
    %set/v v011A77A8_0, 8, 5;
    %movi 8, 5, 4;
    %ix/get 0, 8, 4;
    %delayx 0;
    %load/v 8, v011A77A8_0, 5;
    %set/v v011A7CC8_0, 8, 5;
    %vpi_call 2 48 "$display", "reg_variable_1: %d {time -> %-2d}", v011A7CC8_0, $time;
    %delay 5, 0;
    %movi 8, 3, 5;
    %set/v v011A7D20_0, 8, 5;
    %vpi_call 2 52 "$display", "reg_variable_2: %d {time -> %-2d}", v011A7D20_0, $time;
    %movi 8, 3, 5;
    %set/v v011A7DD0_0, 8, 5;
    %delay 5, 0;
    %load/v 8, v011A7DD0_0, 5;
    %set/v v011A7D78_0, 8, 5;
    %vpi_call 2 58 "$display", "reg_variable_3: %d {time -> %-2d}\012", v011A7D78_0, $time;
    %end;
    .thread T_2;
    .scope S_011A8808;
T_3 ;
