S_011A8808 .scope module, "understand_delay_based_timing_control" "understand_delay_based_timing_control" 2 14;
 .timescale 0 0;
v011A77A8_0 .var *"_s0", 4 0; Local signal
v011AB4C8_0 .var "delay_1", 0 1;
v011A7800_0 .var "delay_2", 0 1;
v011A7858_0 .var "reg_value_1", 0 0;
v011A2B68_0 .var "reg_value_2", 0 0;
v011A2BC0_0 .var "reg_value_3", 0 0;
v011A2C18_0 .var "reg_value_4", 0 0;
