#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01242BF0 .scope module, "understand_multiway_branching" "understand_multiway_branching" 2 7;
 .timescale 0 0;
v01248458_0 .var "reg_variable", 0 1;
E_01249588 .event edge, v01248458_0;
    .scope S_01242BF0;
T_0 ;
    %delay 1, 0;
    %movi 8, 1, 2;
    %set/v v01248458_0, 8, 2;
    %delay 1, 0;
    %movi 8, 2, 2;
    %set/v v01248458_0, 8, 2;
    %delay 1, 0;
    %mov 8, 2, 1;
    %movi 9, 0, 1;
    %set/v v01248458_0, 8, 2;
    %delay 1, 0;
    %mov 8, 3, 1;
    %movi 9, 1, 1;
    %set/v v01248458_0, 8, 2;
    %end;
    .thread T_0;
    .scope S_01242BF0;
T_1 ;
    %wait E_01249588;
    %load/v 8, v01248458_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_1.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_1.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_1.2, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_1.3, 6;
    %movi 10, 0, 1;
    %mov 11, 2, 1;
    %cmp/u 8, 10, 2;
    %jmp/1 T_1.4, 6;
    %movi 12, 1, 1;
    %mov 13, 2, 1;
    %cmp/u 8, 12, 2;
    %jmp/1 T_1.5, 6;
    %mov 14, 2, 1;
    %movi 15, 0, 1;
    %cmp/u 8, 14, 2;
    %jmp/1 T_1.6, 6;
    %mov 16, 2, 1;
    %movi 17, 1, 1;
    %cmp/u 8, 16, 2;
    %jmp/1 T_1.7, 6;
    %cmp/u 8, 2, 2;
    %jmp/1 T_1.8, 6;
    %vpi_call 2 44 "$display", "Invalid input, one of the bits is 'z'.";
    %jmp T_1.10;
T_1.0 ;
    %vpi_call 2 34 "$display", "2'b00";
    %jmp T_1.10;
T_1.1 ;
    %vpi_call 2 35 "$display", "2'b01";
    %jmp T_1.10;
T_1.2 ;
    %vpi_call 2 36 "$display", "2'b10";
    %jmp T_1.10;
T_1.3 ;
    %vpi_call 2 37 "$display", "2'b11";
    %jmp T_1.10;
T_1.4 ;
    %vpi_call 2 38 "$display", "2'bx0";
    %jmp T_1.10;
T_1.5 ;
    %vpi_call 2 39 "$display", "2'bx1";
    %jmp T_1.10;
T_1.6 ;
    %vpi_call 2 40 "$display", "2'b0x";
    %jmp T_1.10;
T_1.7 ;
    %vpi_call 2 41 "$display", "2'b1x";
    %jmp T_1.10;
T_1.8 ;
    %vpi_call 2 42 "$display", "2'bxx";
    %jmp T_1.10;
T_1.10 ;
    %jmp T_1;
    .thread T_1, $push;
# The file index is used to find the file name in the following table.
:file_names 3;
