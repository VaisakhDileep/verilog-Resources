#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_011B2B68 .scope module, "understand_conditional_statements" "understand_conditional_statements" 2 64;
 .timescale 0 0;
v011B2BF0_0 .var/i "integer_value", 31 0;
    .scope S_011B2B68;
T_0 ;
    %movi 8, 5, 32;
    %set/v v011B2BF0_0, 8, 32;
    %end;
    .thread T_0;
    .scope S_011B2B68;
T_1 ;
    %movi 8, 5, 32;
    %load/v 40, v011B2BF0_0, 32;
    %cmp/s 8, 40, 32;
    %jmp/0xz  T_1.0, 5;
    %vpi_call 2 70 "$display", "\042integer_value\042 is greater than 5.";
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v011B2BF0_0, 32;
   %cmpi/s 8, 5, 32;
    %jmp/0xz  T_1.2, 5;
    %vpi_call 2 72 "$display", "\042integer_value\042 is lesser than 5.";
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 74 "$display", "\042integer_value\042 is equal to 5.";
T_1.3 ;
T_1.1 ;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "Program045.v";
