#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01122B08 .scope module, "understand_shift_operators" "understand_shift_operators" 2 7;
 .timescale 0 0;
v01122B90_0 .var "operand_1", 0 4;
v01122BE8_0 .net "operand_2", 0 4, C4<01010>; 1 drivers
    .scope S_01122B08;
T_0 ;
    %movi 8, 17, 5;
    %set/v v01122B90_0, 8, 5;
    %end;
    .thread T_0;
    .scope S_01122B08;
T_1 ;
    %load/v 8, v01122B90_0, 5;
    %ix/load 0, 3, 0;
    %mov 4, 0, 1;
    %shiftr/i0  8, 5;
    %vpi_call 2 21 "$display", "%b >> %1d = %b", v01122B90_0, 3'sb011, T<8,5,u>;
    %load/v 8, v01122B90_0, 5;
    %ix/load 0, 3, 0;
    %mov 4, 0, 1;
    %shiftl/i0  8, 5;
    %vpi_call 2 22 "$display", "%b << %1d = %b\012", v01122B90_0, 3'sb011, T<8,5,u>;
    %load/v 8, v01122BE8_0, 5;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftr/i0  8, 5;
    %vpi_call 2 24 "$display", "%b >> %1d = %b", v01122BE8_0, 2'sb01, T<8,5,u>;
    %load/v 8, v01122BE8_0, 5;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftl/i0  8, 5;
    %vpi_call 2 25 "$display", "%b << %1d = %b", v01122BE8_0, 2'sb01, T<8,5,u>;
    %end;
    .thread T_1;
