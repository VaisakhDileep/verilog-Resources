#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_010E88C8 .scope module, "understand_integer_real_time_register_data_type" "understand_integer_real_time_register_data_type" 2 7;
 .timescale 0 0;
v010EA008_0 .var/i "counter", 31 0;
v010EA060_0 .var "current_time", 63 0;
v010EA0B8_0 .var/real "real_value_1", 0 0;
v010E2AC8_0 .var/i "real_value_1_to_integer", 31 0;
v010E2B20_0 .var/real "real_value_2", 0 0;
v010E2B78_0 .var/i "real_value_2_to_integer", 31 0;
v010E8BC0_0 .var/real "real_value_3", 0 0;
    .scope S_010E88C8;
T_0 ;
    %loadi/wr 4, 1288490188, 4066; load=1.20000
    %loadi/wr 5, 3355443, 4044; load=1.20000
    %add/wr 4, 5;
    %set/wr v010EA0B8_0, 4;
    %end;
    .thread T_0;
    .scope S_010E88C8;
T_1 ;
    %loadi/wr 4, 1932735283, 4066; load=1.80000
    %loadi/wr 5, 838861, 4044; load=1.80000
    %add/wr 4, 5;
    %set/wr v010E2B20_0, 4;
    %end;
    .thread T_1;
    .scope S_010E88C8;
T_2 ;
    %loadi/wr 4, 1250000000, 4101; load=4.00000e+010
    %set/wr v010E8BC0_0, 4;
    %end;
    .thread T_2;
    .scope S_010E88C8;
T_3 ;
    %set/v v010EA008_0, 1, 32;
    %vpi_call 2 30 "$display", "real_value_1: %f", v010EA0B8_0;
    %vpi_call 2 31 "$display", "real_value_2: %f", v010E2B20_0;
    %vpi_call 2 32 "$display", "real_value_3: %f\012", v010E8BC0_0;
    %load/wr 4, v010EA0B8_0;
    %cvt/vr 8, 4, 32;
    %set/v v010E2AC8_0, 8, 32;
    %load/wr 4, v010E2B20_0;
    %cvt/vr 8, 4, 32;
    %set/v v010E2B78_0, 8, 32;
    %vpi_call 2 37 "$display", "real_value_1_to_integer: %d", v010E2AC8_0;
    %vpi_call 2 38 "$display", "real_value_2_to_integer: %d\012", v010E2B78_0;
    %delay 10, 0;
    %vpi_func 2 40 "$time", 8, 64;
    %set/v v010EA060_0, 8, 64;
    %vpi_call 2 42 "$display", "current_time: %t", v010EA060_0;
    %vpi_call 2 44 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "Program008.v";
