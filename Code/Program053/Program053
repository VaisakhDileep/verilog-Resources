#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01382B98 .scope module, "understand_parallel_block" "understand_parallel_block" 2 7;
 .timescale 0 0;
    .scope S_01382B98;
T_0 ;
    %fork t_1, S_01382B98;
    %fork t_2, S_01382B98;
    %fork t_3, S_01382B98;
    %vpi_call 2 27 "$display", "4th {time -> %-2d}", $time;
    %join;
    %join;
    %join;
    %jmp t_0;
t_1 ;
    %vpi_call 2 21 "$display", "1st {time -> %-2d}", $time;
    %end;
t_2 ;
    %vpi_call 2 23 "$display", "2nd {time -> %-2d}", $time;
    %end;
t_3 ;
    %delay 1, 0;
    %vpi_call 2 25 "$display", "3rd {time -> %-2d}", $time;
    %end;
t_0 ;
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "Program053.v";
