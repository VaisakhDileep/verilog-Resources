#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_012D7EE8 .scope module, "understand_event_based_timing_control" "understand_event_based_timing_control" 2 7;
 .timescale 0 0;
v012D7F70_0 .var "bit_condition", 0 0;
v012D7FC8_0 .var "signal", 0 0;
v012D2B68_0 .var "switch_1", 0 0;
v012D2BC0_0 .var "switch_2", 0 0;
v012D2C18_0 .var "switch_3", 0 0;
E_012D9248 .event edge, v012D7F70_0;
E_012D94E8 .event edge, v012D2C18_0, v012D2BC0_0, v012D2B68_0;
E_012D9528 .event negedge, v012D7FC8_0;
E_012D93A8 .event posedge, v012D7FC8_0;
E_012D9328 .event edge, v012D7FC8_0;
E_012D93C8 .event "event_1";
    .scope S_012D7EE8;
T_0 ;
    %wait E_012D9328;
    %vpi_call 2 26 "$display", "\042signal\042 - value changed.";
    %jmp T_0;
    .thread T_0, $push;
    .scope S_012D7EE8;
T_1 ;
    %wait E_012D93A8;
    %vpi_call 2 30 "$display", "\042signal\042 - positive transition.\012";
    %jmp T_1;
    .thread T_1;
    .scope S_012D7EE8;
T_2 ;
    %wait E_012D9528;
    %vpi_call 2 34 "$display", "\042signal\042 - negative transition.\012";
    %jmp T_2;
    .thread T_2;
    .scope S_012D7EE8;
T_3 ;
    %set/v v012D7FC8_0, 1, 1;
    %delay 0, 0;
    %set/v v012D7FC8_0, 0, 1;
    %delay 0, 0;
    %set/v v012D7FC8_0, 1, 1;
    %end;
    .thread T_3;
    .scope S_012D7EE8;
T_4 ;
    %delay 10, 0;
    %set/v E_012D93C8, 0,1;
    %end;
    .thread T_4;
    .scope S_012D7EE8;
T_5 ;
    %wait E_012D93C8;
    %vpi_call 2 55 "$display", "triggering event_1.\012";
    %jmp T_5;
    .thread T_5;
    .scope S_012D7EE8;
T_6 ;
    %wait E_012D94E8;
    %vpi_call 2 63 "$display", "switch_1 or switch_2 or switch_3 was changed.";
    %jmp T_6;
    .thread T_6, $push;
    .scope S_012D7EE8;
T_7 ;
    %delay 20, 0;
    %set/v v012D2B68_0, 1, 1;
    %delay 0, 0;
    %set/v v012D2BC0_0, 0, 1;
    %delay 0, 0;
    %set/v v012D2C18_0, 1, 1;
    %end;
    .thread T_7;
    .scope S_012D7EE8;
T_8 ;
    %set/v v012D7F70_0, 0, 1;
    %end;
    .thread T_8;
    .scope S_012D7EE8;
T_9 ;
T_9.0 ;
    %load/v 8, v012D7F70_0, 1;
    %cmpi/u 8, 1, 1;
    %inv 6, 1;
    %jmp/0xz T_9.1, 6;
    %wait E_012D9248;
    %jmp T_9.0;
T_9.1 ;
    %jmp T_9;
    .thread T_9;
    .scope S_012D7EE8;
T_10 ;
    %delay 30, 0;
    %set/v v012D7F70_0, 1, 1;
    %end;
    .thread T_10;
# The file index is used to find the file name in the following table.
:file_names 3;
