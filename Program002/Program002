#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01108A80 .scope module, "represent_numbers" "represent_numbers" 2 7;
 .timescale 0 0;
v01108B08_0 .var/i "number", 31 0;
    .scope S_01108A80;
T_0 ;
    %movi 8, 3, 32;
    %set/v v01108B08_0, 8, 32;
    %vpi_call 2 27 "$display", "number(decimal): %-d", v01108B08_0;
    %vpi_call 2 28 "$display", "number(binary) : %b\012", v01108B08_0;
    %movi 8, 63, 32;
    %set/v v01108B08_0, 8, 32;
    %vpi_call 2 32 "$display", "number(decimal): %-d", v01108B08_0;
    %vpi_call 2 33 "$display", "number(octal)  : %o\012", v01108B08_0;
    %movi 8, 255, 32;
    %set/v v01108B08_0, 8, 32;
    %vpi_call 2 37 "$display", "number(decimal)    : %-d", v01108B08_0;
    %vpi_call 2 38 "$display", "number(hexadecimal): %h", v01108B08_0;
    %vpi_call 2 39 "$display", "number(hexadecimal): %H\012)", v01108B08_0;
    %movi 8, 255, 32;
    %set/v v01108B08_0, 8, 32;
    %vpi_call 2 43 "$display", "number(decimal): %-d\012", v01108B08_0;
    %movi 8, 2000, 32;
    %set/v v01108B08_0, 8, 32;
    %vpi_call 2 54 "$display", "number(decimal): %-d\012", v01108B08_0;
    %movi 8, 5, 32;
    %set/v v01108B08_0, 8, 32;
    %vpi_call 2 58 "$display", "number(binary): %b\012", v01108B08_0;
    %movi 8, 128, 32;
    %set/v v01108B08_0, 8, 32;
    %vpi_call 2 62 "$display", "number(octal): %o\012", v01108B08_0;
    %movi 8, 658399, 32;
    %set/v v01108B08_0, 8, 32;
    %vpi_call 2 66 "$display", "number(hexadecimal): %h", v01108B08_0;
    %vpi_call 2 68 "$finish";
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "Program002.v";
