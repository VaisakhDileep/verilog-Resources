#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_001F3198 .scope module, "disconnected_level" "disconnected_level" 2 48;
 .timescale 0 0;
S_001F2EA0 .scope module, "l_3" "level_3" 2 50, 2 40, S_001F3198;
 .timescale 0 0;
S_001F3220 .scope module, "root_level" "root_level" 2 10;
 .timescale 0 0;
S_001F2F28 .scope module, "l_1" "level_1" 2 12, 2 20, S_001F3220;
 .timescale 0 0;
S_001F4888 .scope module, "l_2" "level_2" 2 22, 2 30, S_001F2F28;
 .timescale 0 0;
S_001F4910 .scope module, "l_3" "level_3" 2 32, 2 40, S_001F4888;
 .timescale 0 0;
    .scope S_001F2EA0;
T_0 ;
    %vpi_call 2 43 "$display", "Hierarchial level[level_3]           : %m";
    %end;
    .thread T_0;
    .scope S_001F3198;
T_1 ;
    %vpi_call 2 53 "$display", "Hierarchial level[disconnected_level]: %m";
    %end;
    .thread T_1;
    .scope S_001F4910;
T_2 ;
    %vpi_call 2 43 "$display", "Hierarchial level[level_3]           : %m";
    %end;
    .thread T_2;
    .scope S_001F4888;
T_3 ;
    %vpi_call 2 35 "$display", "Hierarchial level[level_2]           : %m";
    %end;
    .thread T_3;
    .scope S_001F2F28;
T_4 ;
    %vpi_call 2 25 "$display", "Hierarchial level[level_1]           : %m";
    %end;
    .thread T_4;
    .scope S_001F3220;
T_5 ;
    %vpi_call 2 15 "$display", "Hierarchial level[root_level]        : %M";
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
