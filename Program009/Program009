#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01237528 .scope module, "understand_arrays" "understand_arrays" 2 7;
 .timescale 0 0;
v010D27B0_0 .var/i "i", 31 0;
v01232A90 .array/i "integer_array_variable_1", 7 0, 31 0;
v01232AE8 .array/i "integer_array_variable_2", 0 7, 31 0;
v01232B40 .array "reg_array_variable", 31 0, 0 0;
    .scope S_01237528;
T_0 ;
    %movi 8, 1, 32;
   %ix/load 1, 0, 0;
   %ix/load 3, 0, 0;
   %set/av v01232A90, 8, 32;
    %movi 8, 1, 32;
   %ix/load 1, 0, 0;
   %ix/load 3, 0, 0;
   %set/av v01232AE8, 8, 32;
   %ix/load 1, 0, 0;
   %ix/load 3, 0, 0;
   %set/av v01232B40, 1, 1;
    %vpi_call 2 26 "$write", "integer_array_variable_1: ";
    %set/v v010D27B0_0, 0, 32;
T_0.0 ;
    %load/v 8, v010D27B0_0, 32;
   %cmpi/s 8, 8, 32;
    %jmp/0xz T_0.1, 5;
    %vpi_call 2 29 "$write", "%-d", &A<v01232A90, v010D27B0_0 >;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v010D27B0_0, 32;
    %set/v v010D27B0_0, 8, 32;
    %jmp T_0.0;
T_0.1 ;
    %vpi_call 2 31 "$write", "\012";
    %vpi_call 2 33 "$write", "integer_array_variable_2: ";
    %set/v v010D27B0_0, 0, 32;
T_0.2 ;
    %load/v 8, v010D27B0_0, 32;
   %cmpi/s 8, 8, 32;
    %jmp/0xz T_0.3, 5;
    %vpi_call 2 36 "$write", "%-d", &A<v01232AE8, v010D27B0_0 >;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v010D27B0_0, 32;
    %set/v v010D27B0_0, 8, 32;
    %jmp T_0.2;
T_0.3 ;
    %vpi_call 2 38 "$write", "\012";
    %vpi_call 2 40 "$write", "reg_array_variable: ";
    %set/v v010D27B0_0, 0, 32;
T_0.4 ;
    %load/v 8, v010D27B0_0, 32;
   %cmpi/s 8, 32, 32;
    %jmp/0xz T_0.5, 5;
    %vpi_call 2 43 "$write", "%-d", &A<v01232B40, v010D27B0_0 >;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v010D27B0_0, 32;
    %set/v v010D27B0_0, 8, 32;
    %jmp T_0.4;
T_0.5 ;
    %vpi_call 2 45 "$write", "\012";
    %end;
    .thread T_0;
